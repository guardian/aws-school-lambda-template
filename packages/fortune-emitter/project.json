{
  "name": "fortune-emitter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/fortune-emitter/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "zip -r ../fortune-emitter.zip *"
        ],
        "cwd": "dist/packages/fortune-emitter"
      },
      "dependsOn": [{
        "projects": "self",
        "target": "transpile"
      }
      ]
    },
    "transpile": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "externalDependencies": "none",
        "outputPath": "dist/packages/fortune-emitter",
        "main": "packages/fortune-emitter/src/main.ts",
        "tsConfig": "packages/fortune-emitter/tsconfig.app.json",
        "assets": ["packages/fortune-emitter/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "packages/fortune-emitter/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "fortune-emitter:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "fortune-emitter:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/fortune-emitter/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/fortune-emitter/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
